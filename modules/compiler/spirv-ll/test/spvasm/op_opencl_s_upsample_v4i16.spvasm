; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int8
                        OpCapability Int16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_upsample_v4i16 "s_upsample_v4i16"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %ui8_t = OpTypeInt 8 0
              %ui16_t = OpTypeInt 16 0
             %v4ui8_t = OpTypeVector %ui8_t 4
             %v4i16_t = OpTypeVector %ui16_t 4
     %s_upsample_fn_t = OpTypeFunction %void_t
                %i8_3 = OpConstant %ui8_t 3
               %ui8_4 = OpConstant %ui8_t 4
              %v4i8_3 = OpConstantComposite %v4ui8_t %i8_3 %i8_3 %i8_3 %i8_3
             %v4ui8_4 = OpConstantComposite %v4ui8_t %ui8_4 %ui8_4 %ui8_4 %ui8_4

; All functions
    %s_upsample_v4i16 = OpFunction %void_t None %s_upsample_fn_t
; CHECK: define spir_kernel void @s_upsample_v4i16()
                   %2 = OpLabel
                   %3 = OpExtInst %v4i16_t %1 s_upsample %v4i8_3 %v4ui8_4
; CHECK: = call spir_func <4 x i16> @_Z8upsampleDv4_cDv4_h(<4 x i8> <i8 3, i8 3, i8 3, i8 3>, <4 x i8> <i8 4, i8 4, i8 4, i8 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
