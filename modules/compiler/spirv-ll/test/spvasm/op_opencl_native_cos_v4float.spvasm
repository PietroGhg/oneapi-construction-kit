; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %native_cos_v4float "native_cos_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
     %native_cos_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.0
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1

; All functions
  %native_cos_v4float = OpFunction %void_t None %native_cos_fn_t
; CHECK: define spir_kernel void @native_cos_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 native_cos %v4float_1
; CHECK: = call spir_func <4 x float> @_Z10native_cosDv4_f(<4 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
