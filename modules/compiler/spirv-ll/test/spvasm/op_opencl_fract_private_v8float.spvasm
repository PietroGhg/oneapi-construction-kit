; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fract_v8float "fract_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
       %v8float_ptr_t = OpTypePointer Function %v8float_t
          %fract_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1

; All functions
       %fract_v8float = OpFunction %void_t None %fract_fn_t
; CHECK: define spir_kernel void @fract_v8float()
                   %2 = OpLabel
                   %3 = OpVariable %v8float_ptr_t Function
; CHECK: [[ptr:%.*]] = alloca <8 x float>
                   %4 = OpExtInst %v8float_t %1 fract %v8float_1 %3
; CHECK: = call spir_func <8 x float> @_Z5fractDv8_fPS_(<8 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr [[ptr]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
