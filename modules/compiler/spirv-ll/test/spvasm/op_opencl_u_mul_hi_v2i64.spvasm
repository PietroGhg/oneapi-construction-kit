; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_mul_hi_v2i64 "u_mul_hi_v2i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
             %v2i64_t = OpTypeVector %i64_t 2
       %u_mul_hi_fn_t = OpTypeFunction %void_t
               %i64_2 = OpConstant %i64_t 2
               %i64_3 = OpConstant %i64_t 3
             %v2i64_2 = OpConstantComposite %v2i64_t %i64_2 %i64_2
             %v2i64_3 = OpConstantComposite %v2i64_t %i64_3 %i64_3

; All functions
      %u_mul_hi_v2i64 = OpFunction %void_t None %u_mul_hi_fn_t
; CHECK: define spir_kernel void @u_mul_hi_v2i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v2i64_t %1 u_mul_hi %v2i64_2 %v2i64_3
; CHECK: = call spir_func <2 x i64> @_Z6mul_hiDv2_mS_(<2 x i64> <i64 2, i64 2>, <2 x i64> <i64 3, i64 3>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
