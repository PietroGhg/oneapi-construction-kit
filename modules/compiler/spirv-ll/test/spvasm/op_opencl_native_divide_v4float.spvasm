; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 %spv_file_s | FileCheck %s
                          OpCapability Kernel
                          OpCapability Addresses
                     %1 = OpExtInstImport "OpenCL.std"
                          OpMemoryModel Physical64 OpenCL
                          OpEntryPoint Kernel %native_divide_v4float "native_divide_v4float"
                          OpSource OpenCL_C 102000

; All types, variables and constants
                %void_t = OpTypeVoid
               %float_t = OpTypeFloat 32
             %v4float_t = OpTypeVector %float_t 4
    %native_divide_fn_t = OpTypeFunction %void_t
               %float_7 = OpConstant %float_t 7.0
              %float_22 = OpConstant %float_t 22.0
             %v4float_7 = OpConstantComposite %v4float_t %float_7 %float_7 %float_7 %float_7
            %v4float_22 = OpConstantComposite %v4float_t %float_22 %float_22 %float_22 %float_22

; All functions
 %native_divide_v4float = OpFunction %void_t None %native_divide_fn_t
; CHECK: define spir_kernel void @native_divide_v4float()
                     %2 = OpLabel
                     %3 = OpExtInst %v4float_t %1 native_divide %v4float_22 %v4float_7
; CHECK: = call spir_func <4 x float> @_Z13native_divideDv4_fS_(<4 x float> <float 2.200000e+01, float 2.200000e+01, float 2.200000e+01, float 2.200000e+01>, <4 x float> <float 7.000000e+00, float 7.000000e+00, float 7.000000e+00, float 7.000000e+00>)
                          OpReturn
; CHECK: ret void
                          OpFunctionEnd
; CHECK: }
