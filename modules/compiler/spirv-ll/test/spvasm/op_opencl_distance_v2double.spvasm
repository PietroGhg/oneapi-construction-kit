; RUN: %if online-spirv-as %{ spirv-as --target-env %spv_tgt_env -o %spv_file_s %s %}
; RUN: %if online-spirv-as %{ spirv-val %spv_file_s %}
; RUN: spirv-ll-tool -a OpenCL -b 64 -c Float64 %spv_file_s | FileCheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %distance_v2double "distance_v2double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v2double_t = OpTypeVector %double_t 2
       %distance_fn_t = OpTypeFunction %void_t
            %double_2 = OpConstant %double_t 2.0
            %double_4 = OpConstant %double_t 4.0
          %v2double_2 = OpConstantComposite %v2double_t %double_2 %double_2
          %v2double_4 = OpConstantComposite %v2double_t %double_4 %double_4

; All functions
   %distance_v2double = OpFunction %void_t None %distance_fn_t
; CHECK: define spir_kernel void @distance_v2double()
                   %2 = OpLabel
                   %3 = OpExtInst %double_t %1 distance %v2double_2 %v2double_4
; CHECK: = call spir_func double @_Z8distanceDv2_dS_(<2 x double> <double 2.000000e+00, double 2.000000e+00>, <2 x double> <double 4.000000e+00, double 4.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
